package service.bean;

import java.util.List;

import domain.Question;

public class QuestionListBean extends Bean {
	private  List<Question> 	QUESTIONDETAIL	;
	private  List<Question> 	QUESTIONDETAIL2	;

	public List<Question> getAnswered() {
		return QUESTIONDETAIL;
	}
	public void setAnswered(List<Question> question) {
		QUESTIONDETAIL = question;
	}

	public List<Question> getUnanswered() {
		return QUESTIONDETAIL2;
	}
	public void setUnanswered(List<Question> question) {
		QUESTIONDETAIL2 = question;
	}
}